solid example

facet normal 0.0 0.0 0.0
  outer loop
    vertex 0.0 0.0 0.0
    vertex 0.0 0.0 1.0
    vertex 0.0 1.0 0.0
  endloop
endfacet

facet normal 0.0 0.0 0.0
  outer loop
    vertex 0.0 1.0 0.0
    vertex 0.0 0.0 1.0
    vertex 0.0 1.0 1.0
  endloop
endfacet

// Add a mountain range
facet normal -0.7 0.0 0.7
  outer loop
    vertex 0.0 0.1 0.1
    vertex 0.0 2.0 0.5
    vertex 0.4 1.0 0.15
  endloop
endfacet

facet normal -0.7 0.0 0.7
  outer loop
    vertex 0.4 1.0 0.15
    vertex 0.0 2.0 0.5
    vertex 1.0 1.3 0.3
  endloop
endfacet

// Add more facets to create a mountain range
facet normal -0.7 0.0 0.7
  outer loop
    vertex 1.0 1.3 0.3
    vertex 0.0 2.0 0.5
    vertex 1.3 0.8 0.2
  endloop
endfacet

facet normal -0.7 0.0 0.7
  outer loop
    vertex 1.3 0.8 0.2
    vertex 0.0 2.0 0.5
    vertex 1.7 1.0 0.15
  endloop
endfacet

// Add more facets to create a mountain range
// ...

endsolid example
